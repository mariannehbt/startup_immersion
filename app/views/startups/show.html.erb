<p id="notice"><%= notice %></p>

<%if @startup.startup_picture.attached?%>
  <%= image_tag @startup.startup_picture, alt: 'startup_picture' %>
<%else%>
  <p>=== Il n'y a pas encore de startup picture lié à cette startup ===</p>
<%end%>
<h3>Changer de startup picture ?</h3>
<%= form_tag startup_startups_pictures_path(@startup), multipart: true do %>
  <%= file_field_tag :startup_picture %>
  <%= submit_tag "mettre à jour" %>
<% end %>

<p>
  <strong>Name:</strong>
  <%= @startup.name %>
</p>

<p>
  <strong>Catch phrase:</strong>
  <%= @startup.catch_phrase %>
</p>

<p>
  <strong>Website url:</strong>
  <%= @startup.website_url %>
</p>

<p>
  <strong>Description:</strong>
  <%= @startup.description %>
</p>

<% if current_user && current_user.admin? %>
  <%= link_to 'Edit', edit_admin_startup_path(@startup) %> |
  <%= link_to 'Back', :startups %>
<% else %>
  <%= link_to 'Back', :startups %>
<% end %>





